<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="onion" match-name="user" />
	<table-filter match-catalog="onion" match-name="id_info" />
	<table-filter match-catalog="onion" match-name="group" />
	<table-filter match-catalog="onion" match-name="role" ></table-filter>
	<table-filter match-catalog="onion" match-name="user_role" ></table-filter>
	
	<table catalog="onion" name="id_info"></table>
	<table catalog="onion" name="group"></table>
	<table catalog="onion" name="user"></table>
	<table catalog="onion" name="role"></table>
	<table catalog="onion" name="user_role"></table>
</hibernate-reverse-engineering>